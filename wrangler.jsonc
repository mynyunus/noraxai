{
    "name": "noraxai",
    "main": "src/worker.js",
    "compatibility_date": "2024-09-23",
    "assets": {
        "directory": "./dist",
        "binding": "ASSETS"
    },
    "build": {
        "command": "npm run build",
        "watch_dir": "src"
    },
    "routes": [
        {
            "pattern": "noraxai.com",
            "custom_domain": true
        },
        {
            "pattern": "www.noraxai.com",
            "custom_domain": true
        }
    ]
}